<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.loquat.datavo.NoticeAdmin" table="NoticeAdmin">
  <id name="noticeId" type="integer">
   <column name="noticeId"/>
   <generator class="native"/>
  </id>
  <property generated="never" lazy="false" name="title" type="string">
   <column name="title" sql-type="VARCHAR(200)"/>
  </property>
  <property generated="never" lazy="false" name="content" type="string">
   <column name="content" sql-type="TEXT"/>
  </property>
  <property generated="never" lazy="false" name="editor" type="string">
   <column name="editor" sql-type="VARCHAR(50)"/>
  </property>
  <property generated="never" lazy="false" name="time" type="timestamp">
   <column name="time" sql-type="TIMESTAMP"/>
  </property>
  <many-to-one class="com.loquat.datavo.SimpleAdmin"
   column="SimpleAdminId" name="admin"/>
  <property name="validTime" type="timestamp">
   <column name="validTime" sql-type="TIMESTAMP"/>
  </property>
 </class>
</hibernate-mapping>

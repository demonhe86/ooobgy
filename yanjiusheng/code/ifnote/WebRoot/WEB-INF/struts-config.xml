<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
  <data-sources />
  <form-beans >
    <form-bean name="loginForm" type="com.ooobgy.ifnote.struts.form.LoginForm" />
    <form-bean name="regForm" type="com.ooobgy.ifnote.struts.form.RegForm" />
    <form-bean name="noteCashForm" type="com.ooobgy.ifnote.struts.form.NoteCashForm" />
    <form-bean name="noteFundForm" type="com.ooobgy.ifnote.struts.form.NoteFundForm" />
    <form-bean name="noteFuturesForm" type="com.ooobgy.ifnote.struts.form.NoteFuturesForm" />
    <form-bean name="noteLoanForm" type="com.ooobgy.ifnote.struts.form.NoteLoanForm" />
    <form-bean name="noteStockForm" type="com.ooobgy.ifnote.struts.form.NoteStockForm" />
    <form-bean name="cashListForm" type="com.ooobgy.ifnote.struts.form.CashListForm" />
    <form-bean name="delNoteForm" type="com.ooobgy.ifnote.struts.form.DelNoteForm" />
    <form-bean name="depositListForm" type="com.ooobgy.ifnote.struts.form.DepositListForm" />
    <form-bean name="fundListForm" type="com.ooobgy.ifnote.struts.form.FundListForm" />
    <form-bean name="futuresListForm" type="com.ooobgy.ifnote.struts.form.FuturesListForm" />
    <form-bean name="loanListForm" type="com.ooobgy.ifnote.struts.form.LoanListForm" />
    <form-bean name="stockListForm" type="com.ooobgy.ifnote.struts.form.StockListForm" />
    <form-bean name="noteDepositForm" type="com.ooobgy.ifnote.struts.form.NoteDepositForm" />
    <form-bean name="summaryForm" type="com.ooobgy.ifnote.struts.form.SummaryForm" />
    <form-bean name="modifyUserPswForm" type="com.ooobgy.ifnote.struts.form.ModifyUserPswForm" />
    <form-bean name="modifyUseInfoForm" type="com.ooobgy.ifnote.struts.form.ModifyUseInfoForm" />

  </form-beans>

  <global-exceptions />
  <global-forwards >
    <forward name="userinfo" path="/component/userInfo.jsp" />
    <forward name="success" path="/success.jsp" />
	<forward name="cashList" path="/user/cashList.jsp" />
	<forward name="depositList" path="/user/depositList.jsp" />
	<forward name="fundList" path="/user/fundList.jsp" />
	<forward name="futuresList" path="/user/futuresList.jsp" />
	<forward name="loanList" path="/user/loanList.jsp" />
	<forward name="stockList" path="/user/stockList.jsp" />
    <forward name="delok" path="/component/delok.jsp" />
    <forward name="error" path="/error/error.html" />
    <forward name="modifyOk" path="/modify/modifyOk.jsp" />
  </global-forwards>

  <action-mappings >
    <action
      attribute="loginForm"
      input="/home/login.jsp"
      name="loginForm"
      path="/login"
      scope="request"
      type="com.ooobgy.ifnote.struts.action.LoginAction">
      <set-property property="cancellable" value="true" />
    </action>
    <action
      attribute="regForm"
      input="/home/reg.jsp"
      name="regForm"
      path="/reg"
      scope="request"
      type="com.ooobgy.ifnote.struts.action.RegAction">
      <set-property property="cancellable" value="true" />
    </action>
    <action
      attribute="noteCashForm"
      input="/inote/noteCash.jsp"
      name="noteCashForm"
      path="/noteCash"
      scope="request"
      type="com.ooobgy.ifnote.struts.action.NoteCashAction">
      <set-property property="cancellable" value="true" />
    </action>
    <action
      attribute="noteFundForm"
      input="/inote/noteFund.jsp"
      name="noteFundForm"
      path="/noteFund"
      scope="request"
      type="com.ooobgy.ifnote.struts.action.NoteFundAction">
      <set-property property="cancellable" value="true" />
    </action>
    <action
      attribute="noteFuturesForm"
      input="/inote/noteFutures.jsp"
      name="noteFuturesForm"
      path="/noteFutures"
      scope="request"
      type="com.ooobgy.ifnote.struts.action.NoteFuturesAction">
      <set-property property="cancellable" value="true" />
    </action>
    <action
      attribute="noteLoanForm"
      input="/inote/noteLoan.jsp"
      name="noteLoanForm"
      path="/noteLoan"
      scope="request"
      type="com.ooobgy.ifnote.struts.action.NoteLoanAction">
      <set-property property="cancellable" value="true" />
    </action>
    <action
      attribute="noteStockForm"
      input="/inote/noteStock.jsp"
      name="noteStockForm"
      path="/noteStock"
      scope="request"
      type="com.ooobgy.ifnote.struts.action.NoteStockAction">
      <set-property property="cancellable" value="true" />
    </action>
    <action
      attribute="cashListForm"
      input="/user/cashList.jsp"
      name="cashListForm"
      path="/cashList"
      scope="request"
      type="com.ooobgy.ifnote.struts.action.CashListAction">
      
      <set-property property="cancellable" value="true" />
    </action>
    <action
      attribute="delNoteForm"
      input="/component/delok.jsp"
      name="delNoteForm"
      path="/delNote"
      scope="request"
      type="com.ooobgy.ifnote.struts.action.DelNoteAction">
      <set-property property="cancellable" value="true" />
    </action>
    <action
      attribute="depositListForm"
      input="/user/depositList.jsp"
      name="depositListForm"
      path="/depositList"
      scope="request"
      type="com.ooobgy.ifnote.struts.action.DepositListAction">
      <set-property property="cancellable" value="true" />
    </action>
    <action
      attribute="fundListForm"
      input="/user/fundList.jsp"
      name="fundListForm"
      path="/fundList"
      scope="request"
      type="com.ooobgy.ifnote.struts.action.FundListAction">
      <set-property property="cancellable" value="true" />
    </action>
    <action
      attribute="futuresListForm"
      input="/user/futuresList.jsp"
      name="futuresListForm"
      path="/futuresList"
      scope="request"
      type="com.ooobgy.ifnote.struts.action.FuturesListAction">
      <set-property property="cancellable" value="true" />
    </action>
    <action
      attribute="loanListForm"
      input="/user/loanList.jsp"
      name="loanListForm"
      path="/loanList"
      scope="request"
      type="com.ooobgy.ifnote.struts.action.LoanListAction">
      <set-property property="cancellable" value="true" />
    </action>
    <action
      attribute="stockListForm"
      input="/user/stockList.jsp"
      name="stockListForm"
      path="/stockList"
      scope="request"
      type="com.ooobgy.ifnote.struts.action.StockListAction">
      <set-property property="cancellable" value="true" />
    </action>
    <action
      attribute="noteDepositForm"
      input="/inote/noteDeposit.jsp"
      name="noteDepositForm"
      path="/noteDeposit"
      scope="request"
      type="com.ooobgy.ifnote.struts.action.NoteDepositAction">
      <set-property property="cancellable" value="true" />
    </action>
    <action
      attribute="summaryForm"
      input="/user/summary.jsp"
      name="summaryForm"
      path="/summary"
      scope="request"
      type="com.ooobgy.ifnote.struts.action.SummaryAction">
      <set-property property="cancellable" value="true" />
    </action>
    <action
      attribute="modifyUserPswForm"
      input="/modify/modifyUserPsw.jsp"
      name="modifyUserPswForm"
      path="/modifyUserPsw"
      scope="request"
      type="com.ooobgy.ifnote.struts.action.ModifyUserPswAction">
      <set-property property="cancellable" value="true" />
    </action>
    <action
      attribute="modifyUseInfoForm"
      input="/modify/modifyUseInfo.jsp"
      name="modifyUseInfoForm"
      path="/modifyUseInfo"
      scope="request"
      type="com.ooobgy.ifnote.struts.action.ModifyUseInfoAction">
      <set-property property="cancellable" value="true" />
    </action>
    

  </action-mappings>

  <message-resources parameter="com.ooobgy.ifnote.struts.ApplicationResources" />
</struts-config>


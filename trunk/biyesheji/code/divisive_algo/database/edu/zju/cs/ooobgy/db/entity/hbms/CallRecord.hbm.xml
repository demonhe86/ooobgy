<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="edu.zju.cs.ooobgy.db.entity.CallRecord" table="call_record">
  <id name="cid" type="integer">
   <column name="cid" not-null="true" sql-type="INTEGER" unique="true"/>
   <generator class="native"/>
  </id>
  <property generated="never" lazy="false" name="callerNum" type="string">
   <column name="caller_num" sql-type="VARCHAR(50)"/>
  </property>
  <property generated="never" lazy="false" name="receiverNum" type="string">
   <column name="receiver_num" sql-type="VARCHAR(50)"/>
  </property>
  <property name="startTime" type="timestamp">
   <column name="start_time" sql-type="TIMESTAMP"/>
  </property>
  <property generated="never" lazy="false" name="stationId" type="string">
   <column name="station_id" sql-type="VARCHAR(50)"/>
  </property>
  <property generated="never" lazy="false" name="stationCode" type="string">
   <column name="station_code" sql-type="VARCHAR(50)"/>
  </property>
  <property name="callDuration" type="double">
   <column name="call_duration" sql-type="DOUBLE"/>
  </property>
 </class>
</hibernate-mapping>

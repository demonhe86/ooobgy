<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
  <form-beans >
    <form-bean name="makeDatabaseForm" type="com.ooobgy.struts.form.MakeDatabaseForm" />
    <form-bean name="makeDataForm" type="com.ooobgy.struts.form.MakeDataForm" />

  </form-beans>

  <global-exceptions />
  <global-forwards />
  <action-mappings >
    <action
      attribute="makeDatabaseForm"
      input="/superAdmin/makeDatabase.jsp"
      name="makeDatabaseForm"
      path="/makeDatabase"
      scope="request"
      type="com.ooobgy.struts.action.MakeDatabaseAction"
      cancellable="true" />
    <action
      attribute="makeDataForm"
      input="/makeData.jsp"
      name="makeDataForm"
      path="/makeData"
      scope="request"
      type="com.ooobgy.struts.action.MakeDataAction"
      cancellable="true" />

  </action-mappings>

  <message-resources parameter="com.ooobgy.struts.ApplicationResources" />
</struts-config>

